{
  "parser": "babel-eslint",
  "extends": ["standard", "prettier"],
  "env": {
    "browser": true
  },
  "rules": {
    "max-len": ["error", { "code": 130 }],
    "no-console": "off",
    "no-underscore-dangle": "off",
    "import/prefer-default-export": "off",
    "import/no-cycle": "off",
    "consistent-return": "off"
  }
}
